@model StudentManagerMVC.Models.StudentModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h3 class="text-center p-4">Info Student</h3>
<hr />
<div class="d-flex flex-column align-items-center">
    <div class="form-group p-2">
        <label asp-for="id" class="control-label"></label>
        <input disabled asp-for="id" class="form-control" />
    </div>
    <div class="form-group p-2">
        <label asp-for="name" class="control-label"></label>
        <input disabled asp-for="name" class="form-control" />
    </div>
    <div class="form-group p-2">
        <label asp-for="email" class="control-label"></label>
        <input disabled asp-for="email" class="form-control" />
    </div>
    <div class="form-group p-2">
        <label asp-for="stream" class="control-label"></label>
        <input disabled asp-for="stream" class="form-control" />
    </div>
    <a asp-action="Index" class="btn btn-outline-info mt-4 align-self-end">Back</a>
</div>